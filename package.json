{
  "name": "creature-api-boilerplate",
  "description": "A Boilerplate for creatures APIs",
  "version": "0.2.0",
  "homepage": "https://github.com/apply-creatures/creature-api-boilerplate",
  "license": "MIT",
  "main": "src",
  "keywords": [
    "feathersjs",
    "API",
    "REST",
    "nodejs",
    "typescript",
    "sequelize",
    "postgres",
    "biome",
    "swagger",
    "redocly",
    "winston"
  ],
  "author": {
    "name": "Hirako2000",
    "email": "hirako2000@gmail.com"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/apply-creatures/creature-api-boilerplate/issues"
  },
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/",
    "doc": "doc"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "scripts": {
    "test": "jest --forceExit",
    "dev": "ts-node-dev --no-notify src/",
    "lint:check": "npx @biomejs/biome check ./src",
    "lint": "npx @biomejs/biome check --write ./src",
    "start": "yarn run compile && node lib/",
    "start:prod": "node lib/",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@bervproject/feathers-advance-hook": "^2.0.1",
    "@feathersjs/authentication": "^5.0.26",
    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "5.0.26",
    "@feathersjs/authentication-oauth": "5.0.26",
    "@feathersjs/configuration": "5.0.26",
    "@feathersjs/errors": "5.0.9",
    "@feathersjs/express": "5.0.25",
    "@feathersjs/feathers": "5.0.26",
    "@feathersjs/socketio": "5.0.26",
    "@google-cloud/storage": "7.11.1",
    "cls-hooked": "4.2.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "feathers-hooks-common": "8.1.2",
    "feathers-sequelize": "7.0.3",
    "feathers-swagger": "3.0.0",
    "helmet": "7.1.0",
    "mime-types": "2.1.35",
    "multer": "1.4.5-lts.1",
    "pg": "8.11.5",
    "redoc": "^2.1.5",
    "sequelize": "6.37.3",
    "serve-favicon": "2.5.0",
    "swagger-ui-dist": "5.17.14",
    "uuid": "9.0.1",
    "winston": "3.13.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@types/bluebird": "^3.5.42",
    "@types/cls-hooked": "^4.3.8",
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.15",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mime-types": "2.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.13.0",
    "@types/serve-favicon": "^2.5.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.11.10",
    "axios": "^1.7.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.2",
    "shx": "^0.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@1.22.19"
}
